<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".professionalregister2">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="530dp"
        android:src="@drawable/free"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="161dp"
        android:layout_height="16dp"
        android:layout_marginStart="99dp"
        android:layout_marginTop="359dp"
        android:layout_marginEnd="99dp"
        android:layout_marginBottom="346dp"
        android:background="#000000"
        android:fontFamily="@font/josefinsansregular"
        android:text="Choose Your Profession"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <CheckedTextView
        android:id="@+id/checkedTextView"
        android:layout_width="266dp"
        android:layout_height="40dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="393dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="287dp"
        android:background="@drawable/edittext_bg"
        android:fontFamily="@font/josefinsansregular"
        android:gravity="center_vertical"
        android:text="   Working/Owner"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckedTextView
        android:id="@+id/checkedTextView2"
        android:layout_width="266dp"
        android:layout_height="40dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="47dp"
        android:background="@drawable/edittext_bg"
        android:fontFamily="@font/josefinsansregular"
        android:gravity="center_vertical"
        android:text="   Freelancer"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkedTextView" />

    <CheckedTextView
        android:id="@+id/checkedTextView3"
        android:layout_width="266dp"
        android:layout_height="40dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="47dp"
        android:background="@drawable/edittext_bg"
        android:fontFamily="@font/josefinsansregular"
        android:gravity="center_vertical"
        android:text="   Teacher/HoD/Instructor"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkedTextView2" />

    <Button
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="76dp"
        android:background="@drawable/button_bg"
        android:text="Countinue"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkedTextView3" />


</androidx.constraintlayout.widget.ConstraintLayout>